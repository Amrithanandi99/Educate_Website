
<body>
  <%- include('header.ejs') %>
<div class="container-fluid row">
<div class="col-3">
  <%- include('left.ejs') %>  
</div>

<div class="col-9">
  <div class="container mt-5">
      <h2>Contact Us</h2>
      <form method="post">
          <div class="form-group">
              <label for="name">Name</label>
              <input type="text" class="form-control" name="name"  value="<%= contactdata.name %>"  placeholder="Enter your name">
          </div>
          <div class="form-group">
              <label for="email">Email</label>
              <input type="email" class="form-control" name="email" value="<%= contactdata.email %>" placeholder="Enter your email">
          </div>
          <div class="form-group">
              <label for="message">Message</label>
              <textarea class="form-control" name="message" value="<%= contactdata.message %>" rows="4" placeholder="Enter your message"></textarea>
          </div>
          <button type="submit" class="btn btn-orange">Submit</button>
      </form>
  </div>

  
 